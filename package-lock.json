{
  "name": "monorepo",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "monorepo",
      "workspaces": [
        "packages/*"
      ]
    },
    "node_modules/@monorepo/backend": {
      "resolved": "packages/backend",
      "link": true
    },
    "node_modules/@monorepo/frontend": {
      "resolved": "packages/frontend",
      "link": true
    },
    "packages/backend": {
      "name": "@monorepo/backend",
      "version": "1.0.0",
      "license": "ISC"
    },
    "packages/frontend": {
      "name": "@monorepo/frontend",
      "version": "1.0.0",
      "license": "ISC",
      "dependencies": {
        "@monorepo/backend": "1.0.0"
      }
    }
  },
  "dependencies": {
    "@monorepo/backend": {
      "version": "file:packages/backend"
    },
    "@monorepo/frontend": {
      "version": "file:packages/frontend",
      "requires": {
        "@monorepo/backend": "1.0.0"
      }
    }
  }
}
